#!/usr/bin/env python3
"""
Shinka Visualization CLI

Command-line interface for visualizing Shinka evolution results.
"""

import os
import sys

# Add the parent directory to the Python path to ensure shinka can be imported
sys.path.insert(0, os.path.dirname(os.path.dirname(os.path.abspath(__file__))))

from shinka.webui.visualization import main

if __name__ == "__main__":
    main()
